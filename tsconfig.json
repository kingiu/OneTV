{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020"],
    "allowJs": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["expo-env.d.ts"],
  "exclude": ["node_modules"]
}